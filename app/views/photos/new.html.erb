<h1>Upload a New Photo</h1>
<%= "Error:" + @photo.errors.full_messages.to_sentence if @photo.errors.any? %><br>
<%= form_for @photo, url: photos_path, html: { multipart: true } do |form| %>
  <%= label_tag :photo_grad_year, "Grad year (Comma separate years if photo has multiple years):" %>
  <%= text_field(:photo, :grad_year, in: 1800..2100) %><br>
  <%= form.file_field :image %><br>
  <%= submit_tag "Submit" %>
<% end %>
<h2><%= link_to "Back to All Photos", photos_path %></h2>
